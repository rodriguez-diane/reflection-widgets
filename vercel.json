{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/wins", "dest": "api/wins.js" },
    { "src": "/api/synthesis", "dest": "api/synthesis.js" },
    { "src": "/", "dest": "public/index.html" },
    { "src": "/synthesis", "dest": "public/synthesis.html" }
  ]
}
